  #if compiler(>=5.3) && $NonescapableTypes && $AsyncExecutionBehaviorAttributes
  nonisolated(nonsending) final public func getRecommendations(for placement: AlFinderSDK.AlFinderClient.RecommendationPlacement, productIds: [Swift.String]? = nil, lang: AlFinderSDK.AlFinderClient.Lang? = nil) async throws -> AlFinderSDK.AlFinderClient.RecommendationsResponse
  #endif
}
extension AlFinderSDK.AlFinderClient {
  public enum RecommendationPlacement : Swift.String, Swift.Decodable, Swift.Sendable, Swift.Hashable {
    case productDetailsPage
    case homePage
    case cartPage
    #if compiler(>=5.3) && $NonescapableTypes
    public init?(rawValue: Swift.String)
    #endif
    public typealias RawValue = Swift.String
    public var rawValue: Swift.String {
      get
    }
  }
  public enum RecommendationType : Swift.String, Swift.Decodable, Swift.Sendable, Swift.Hashable {
    case relatedProducts
    case aiComplementary
    case bestSeller
    case recentlyViewed
    case trendingProducts
    case recentlyAdded
    case mostViewed
    case recentlyPurchased
    case frequentlyBoughtTogether
    #if compiler(>=5.3) && $NonescapableTypes
    public init?(rawValue: Swift.String)
    #endif
    public typealias RawValue = Swift.String
    public var rawValue: Swift.String {
      get
    }
  }
  public struct ActiveRecommendationsResponse : Swift.Decodable, Swift.Sendable {
    public let map: [AlFinderSDK.AlFinderClient.RecommendationPlacement : [AlFinderSDK.AlFinderClient.RecommendationType]]
    public init(from decoder: any Swift.Decoder) throws
    public func types(for placement: AlFinderSDK.AlFinderClient.RecommendationPlacement) -> [AlFinderSDK.AlFinderClient.RecommendationType]
  }
  public struct RecommendationsResponse : Swift.Decodable, Swift.Sendable {
    public let results: [AlFinderSDK.AlFinderClient.RecommendationBlock]
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct RecommendationBlock : Swift.Decodable, Swift.Sendable {
    public let recommendation: AlFinderSDK.AlFinderClient.RecommendationConfig
    public let products: [AlFinderSDK.AlFinderClient.Product]
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct RecommendationConfig : Swift.Decodable, Swift.Sendable {
    public let pageType: AlFinderSDK.AlFinderClient.RecommendationPlacement
    public let type: AlFinderSDK.AlFinderClient.RecommendationType
    public let title: Swift.String?
    public let titleEn: Swift.String?
    public let titlePosition: Swift.String?
    public init(from decoder: any Swift.Decoder) throws
  }
}
