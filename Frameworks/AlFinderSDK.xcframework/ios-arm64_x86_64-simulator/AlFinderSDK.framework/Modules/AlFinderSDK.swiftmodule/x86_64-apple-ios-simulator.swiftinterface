    }
  }
}
public enum SDKError : Swift.Error, Swift.Sendable {
  case notConfigured
  case invalidURL
  case invalidResponse
  case httpError(status: Swift.Int, body: Swift.String?)
  case decoding(any Swift.Error)
}
