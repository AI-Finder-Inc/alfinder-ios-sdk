extension AlFinderSDK.AlFinderClient {
  public struct SearchProductsResponse : Swift.Decodable, Swift.Sendable {
    public let results: AlFinderSDK.AlFinderClient.Results
    public let meta: AlFinderSDK.AlFinderClient.Meta
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Results : Swift.Decodable, Swift.Sendable {
    public let products: [AlFinderSDK.AlFinderClient.Product]
    public let filters: [AlFinderSDK.AlFinderClient.Filter]
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Product : Swift.Decodable, Swift.Sendable {
    public let id: Swift.Int?
    public let productId: Swift.String?
    public let name: AlFinderSDK.AlFinderClient.LocalizedText
    public let description: AlFinderSDK.AlFinderClient.LocalizedText?
    public let type: Swift.String?
    public let isTaxable: Swift.Bool?
    public let sku: Swift.String?
    public let price: Swift.Double?
    public let discountedPrice: Swift.Double?
    public let discountStartDate: Swift.String?
    public let discountEndDate: Swift.String?
    public let currency: Swift.String?
    public let url: Swift.String?
    public let urlTrans: AlFinderSDK.AlFinderClient.LocalizedURL?
    public let mainImage: Swift.String?
    public let isAvailable: Swift.Bool?
    public let categories: [AlFinderSDK.AlFinderClient.Category]?
    public let media: [AlFinderSDK.AlFinderClient.Media]?
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct LocalizedText : Swift.Decodable, Swift.Sendable {
    public let ar: Swift.String?
    public let en: Swift.String?
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct LocalizedURL : Swift.Decodable, Swift.Sendable {
    public let ar: Swift.String?
    public let en: Swift.String?
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Category : Swift.Decodable, Swift.Sendable {
    public let name: AlFinderSDK.AlFinderClient.LocalizedText
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Media : Swift.Decodable, Swift.Sendable {
    public let url: Swift.String?
    public let type: Swift.String?
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Filter : Swift.Decodable, Swift.Sendable {
    public init()
    public init(from decoder: any Swift.Decoder) throws
  }
  public struct Meta : Swift.Decodable, Swift.Sendable {
    public let pageNumber: Swift.Int?
    public let pageSize: Swift.Int?
    public let prevPage: Swift.Int?
    public let nextPage: Swift.Int?
    public let total: Swift.Int?
    public let totalPages: Swift.Int?
    public init(from decoder: any Swift.Decoder) throws
  }
}
@_hasMissingDesignatedInitializers final public class AlFinderClient : @unchecked Swift.Sendable {
  public static let shared: AlFinderSDK.AlFinderClient
  public struct Configuration : Swift.Sendable {
    public let baseURL: Foundation.URL
    public let storeId: Swift.String
    public let provider: AlFinderSDK.AlFinderClient.Provider
    public let timeout: Foundation.TimeInterval
    public let defaultLang: AlFinderSDK.AlFinderClient.Lang
    public let storeURL: Foundation.URL
    public init(baseURL: Foundation.URL = URL(string: "https://widget.alfinder.ai/v1/stores")!, storeId: Swift.String, provider: AlFinderSDK.AlFinderClient.Provider, timeout: Foundation.TimeInterval = 30, defaultLang: AlFinderSDK.AlFinderClient.Lang = .en)
  }
  public static func configure(baseURL: Foundation.URL = URL(string: "https://widget-staging.alfinder.ai/v1/stores")!, storeId: Swift.String, provider: AlFinderSDK.AlFinderClient.Provider, timeout: Foundation.TimeInterval = 30, defaultLang: AlFinderSDK.AlFinderClient.Lang = .en)
  @objc deinit
}
